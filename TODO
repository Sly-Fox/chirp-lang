func MkEnsemble(name string, items []EnsembleItem) Command {
	cnd := func(fr *Frame, argv []T) T {
		switch len(argv) {
		case 0:
			panic("TODO: doc string")
		case 1:
			panic("Ensemble command needs a subcommand argument: " + Showv(argv))
		}
		subName := argv[1]
		for _, e := range items {
			if e.Name == subName {
				return e.Cmd(argv[1:])
			}
		}
		panic("Ensemble subcommand not found: " + Showv(argv))
	}
	return cmd
}
type EnsembleItem struct {
	Name	string
	Cmd		Command	
	Doc		string
}

